
  <div id="<%= dom_id client %>">

<table class="table table-bordered text-start mt-5 table-hover">
      <thead>
        <tr class="text-center">
          <th scope="col">#</th>
          <th scope="col">Name</th>
          <th scope="col">Age</th>
        </tr>
      </thead>
      <tbody>
          <tr>
            <th scope="row"><%= client.id %></th>
            <td><%= client.name %></td>
            <td><%= client.age %></td>
          </tr>
      </tbody>
    </table>
</div>
